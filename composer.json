{
    "name": "oro/commerce-cybersource",
    "description": "OroCommerce CyberSource Integration",
    "homepage": "https://github.com/oroinc/cybersource.git",
    "license": "OSL-3.0",
    "keywords": ["Oro", "CyberSource", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://www.orocommerce.com/"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\CyberSourceBundle\\": "src/Oro/Bundle/CyberSourceBundle"},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "oro/commerce": "5.0.*",
        "cybersource/rest-client-php": "dev-main"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
	"cybersource-rest-client-php": {
            "type": "vcs",
            "url": "https://github.com/oroinc/cybersource-rest-client-php"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
